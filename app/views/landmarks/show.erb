<table>
  <tr>
    <th>Name</th>
    <th>Year Completed</th>
  </tr>
  <tr>
    <td><%= @landmark.name %></td>
    <td><%= @landmark.year_completed %></td>
  </tr>
</table>

<form method="post" action="/landmarks/<%= @landmark.id %>">
  <input type="hidden" name="_method" value="delete" />
  <input type="submit" value="Delete Landmark" />
</form>
